{
  "name": "redflow-designer",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"webflow extension serve\" \"pnpm run watch-webpack\"",
    "watch-webpack": "webpack --config webpack.config.mjs --mode development --watch",
    "build": "pnpm run build-webpack && webflow extension bundle",
    "build-webpack": "webpack --config webpack.config.mjs --mode production"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@webflow/designer-extension-typings": "^2.0.2",
    "typescript": "^5.4.5",
    "ts-loader": "^9.5.1",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "concurrently": "^8.2.2"
  }
}

{
  "name": "myapp",
  "version": "1.0.0",
  "scripts": {
    "dev": "pnpm install && concurrently \"webflow extension serve\" \"pnpm run watch-webpack\"",
    "build": "pnpm run build-webpack && webflow extension bundle",
    "watch-webpack": "webpack --config webpack.config.mjs --mode development --watch",
    "build-webpack": "webpack --config webpack.config.mjs --mode production",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "devDependencies": {
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@webflow/designer-extension-typings": "^2.0.12",
    "@workspace/typescript-config": "workspace:*",
    "concurrently": "^9.1.2",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
